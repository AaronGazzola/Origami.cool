{
  "name": "origami.cool",
  "version": "1.0.0",
  "description": "eCommerce web app for hand folded 3D origami models",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AaronGazzola/Origami.cool.git"
  },
  "keywords": [
    "origami",
    "ecommerce",
    "MERN"
  ],
  "author": "Aaron Gazzola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AaronGazzola/Origami.cool/issues"
  },
  "homepage": "https://github.com/AaronGazzola/Origami.cool#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-mongo-sanitize": "^2.0.2",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.18",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "sanitize-html": "^2.3.2",
    "slugify": "^1.4.7",
    "xss-clean": "^0.1.1"
  }
}
